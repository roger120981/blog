{
  "name": "@blog/frontend",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "opt-imgs": "node src/lib/sharp/optimizeImages.js",
    "type-check": "astro check && tsc --noEmit --skipLibCheck",
    "link-check": "linkinator http://localhost:3000 -r -s adidas,butora,linkedin",
    "a11y-check": "pa11y-ci --sitemap https://tonydang.blog/sitemap-index.xml --sitemap-find https://tonydang.blog --sitemap-replace http://localhost:3000",
    "build": "pnpm run type-check && astro build && pnpm run link-check && pnpm run a11y-check",
    "preview": "wrangler pages dev --compatibility-date=2023-03-14 --port 4000 dist",
    "deploy": "wrangler pages deploy dist"
  },
  "dependencies": {
    "@astrojs/mdx": "^1.0.3",
    "@astrojs/prefetch": "^0.4.0",
    "@astrojs/react": "^3.0.2",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.0",
    "@astrojs/svelte": "^4.0.2",
    "@astrojs/tailwind": "^5.0.0",
    "@dimforge/rapier2d-compat": "^0.11.2",
    "@dimforge/rapier3d-compat": "^0.11.2",
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@react-three/drei": "^9.83.9",
    "@react-three/fiber": "^8.14.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tonydangblog/error-handling": "workspace:*",
    "astro": "^3.0.13",
    "astro-seo": "^0.8.0",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "gsap": "^3.12.2",
    "leva": "^0.9.35",
    "lil-gui": "^0.18.2",
    "plotly.js-dist-min": "^2.26.0",
    "postgres": "^3.3.5",
    "r3f-perf": "^7.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-toc": "^8.0.1",
    "sharp": "^0.32.5",
    "stats.js": "^0.17.0",
    "svelte": "^4.2.0",
    "tailwindcss": "^3.3.3",
    "three": "^0.154.0",
    "vite-plugin-glsl": "^1.1.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.0",
    "@types/d3": "^7.4.0",
    "@types/plotly.js-dist-min": "^2.3.1",
    "@types/react": "^18.2.21",
    "@types/three": "^0.154.0",
    "glob": "^10.3.4",
    "linkinator": "^5.0.2",
    "pa11y-ci": "^3.0.1"
  }
}
